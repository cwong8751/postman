# Ignore Composer dependencies (vendor should not be committed)
vendor/

# Ignore Composer lock file (optional, but recommended for libraries)
composer.lock

# Ignore system files and caches
.DS_Store
Thumbs.db
*.log
*.cache
*.tmp
*.swp
*.swo

# Ignore environment & secret files
.env
.env.local
.env.*.local
!.env.example

# Ignore IDE/editor-specific files
.vscode/
.idea/
*.iml

# Ignore PHPUnit and coverage reports
phpunit.xml
phpunit.xml.dist
/build/
coverage/
phpunit-result.log

# Ignore Laravel-specific files (if using Laravel)
storage/
bootstrap/cache/
public/storage
.env.testing

# Ignore Symfony-specific files (if using Symfony)
var/
!var/.gitkeep

# Ignore WordPress-specific files (if using WordPress)
wp-content/uploads/
wp-content/upgrade/
wp-config.php
wp-config-local.php

# Ignore cache & logs
*.log
logs/
cache/
debug.log

# Ignore Node modules (if using frontend dependencies)
node_modules/
package-lock.json
yarn.lock

# Ignore dependencies from npm, yarn or pnpm (if applicable)
.pnpmfile.cjs
.pnpm-debug.log

# Ignore built assets (if applicable)
public/build/
public/assets/
dist/
